cv::Mat<sp/>ImageProcess::cameraUndistort(cv::Mat<sp/>src)
{
<sp/><sp/>double<sp/>ma[3][3]<sp/>=<sp/>{
<sp/><sp/><sp/><sp/><sp/><sp/><sp/><sp/>5.465101556178353e+02,
<sp/><sp/><sp/><sp/><sp/>0.650701463159305,
<sp/><sp/><sp/><sp/><sp/>6.491732555284723e+02,
<sp/><sp/><sp/><sp/><sp/>0,
<sp/><sp/><sp/><sp/><sp/>5.460864354608683e+02,
<sp/><sp/><sp/><sp/><sp/>3.503090793118121e+02,
<sp/><sp/><sp/><sp/><sp/>0,
<sp/><sp/><sp/><sp/><sp/>0,
<sp/><sp/><sp/><sp/><sp/>1
<sp/><sp/>};<sp/>///&lt;<sp/>for<sp/>1280*720

<sp/><sp/>Mat<sp/>cameraMatrix<sp/>=<sp/>Mat(3,<sp/>3,<sp/>CV_64F,<sp/>ma);
