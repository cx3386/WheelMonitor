cv::Mat<sp/>ImageProcess::cameraUndistort(cv::Mat<sp/>src)
{
<sp/><sp/>double<sp/>ma[3][3]<sp/>=<sp/>{
<sp/><sp/><sp/><sp/><sp/><sp/><sp/><sp/>5.465101556178353e+02,
<sp/><sp/><sp/><sp/><sp/>0.650701463159305,
<sp/><sp/><sp/><sp/><sp/>6.491732555284723e+02,
<sp/><sp/><sp/><sp/><sp/>0,
<sp/><sp/><sp/><sp/><sp/>5.460864354608683e+02,
<sp/><sp/><sp/><sp/><sp/>3.503090793118121e+02,
<sp/><sp/><sp/><sp/><sp/>0,
<sp/><sp/><sp/><sp/><sp/>0,
<sp/><sp/><sp/><sp/><sp/>1
<sp/><sp/>};<sp/>///&lt;<sp/>for<sp/>1280*720

<sp/><sp/>Mat<sp/>cameraMatrix<sp/>=<sp/>Mat(3,<sp/>3,<sp/>CV_64F,<sp/>ma);
<sp/><sp/>Mat<sp/>distCoeffs<sp/>=<sp/>(Mat_&lt;double&gt;(5,<sp/>1)<sp/>&lt;&lt;<sp/>-0.126396146351086,
<sp/><sp/><sp/><sp/><sp/><sp/><sp/><sp/>0.012067785981004,
<sp/><sp/><sp/><sp/><sp/>-6.004717303426694e-04,
<sp/><sp/><sp/><sp/><sp/><sp/><sp/><sp/>0.001281258711954,
<sp/><sp/><sp/><sp/><sp/>0);

<sp/><sp/><sp/>Mat<sp/>view,<sp/>rview,<sp/>map1,<sp/>map2;
<sp/><sp/><sp/>Size<sp/>imageSize<sp/>=<sp/>src.size();

<sp/><sp/>//undistort<sp/>combine<sp/>initUndistortRectifyMap<sp/>and<sp/>remap
<sp/><sp/>initUndistortRectifyMap(cameraMatrix,<sp/>distCoeffs,<sp/>Mat(),
<sp/><sp/><sp/><sp/><sp/><sp/><sp/>getOptimalNewCameraMatrix(cameraMatrix,<sp/>distCoeffs,<sp/>imageSize,<sp/>1,<sp/>imageSize,<sp/>nullptr),
<sp/><sp/><sp/><sp/><sp/>imageSize,<sp/>CV_16SC2,<sp/>map1,<sp/>map2);

<sp/>Mat<sp/>srcCalibration;
<sp/><sp/><sp/><sp/>remap(src,<sp/>srcCalibration,<sp/>map1,<sp/>map2,<sp/>INTER_LINEAR);
<sp/><sp/>return<sp/>srcCalibration;
}

