void<sp/>ConfigHelper::save()
{
<sp/><sp/><sp/>auto<sp/>settings<sp/>=<sp/>new<sp/>QSettings(configFile,<sp/>QSettings::IniFormat,<sp/>this);
<sp/>takeEffect();
<sp/><sp/>settings-&gt;beginGroup(GP_COMMON);
<sp/><sp/><sp/>settings-&gt;setValue(&quot;launchAtLogin&quot;,<sp/>QVariant(launchAtLogin));
<sp/><sp/>settings-&gt;setValue(&quot;startAtLaunch&quot;,<sp/>QVariant(startAtLaunch));
